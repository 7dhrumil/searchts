	"author": "Mineru (https://github.com/Mineru98)",
	"license": "MIT",
	"contributors": [
		{
			"name": "Mineru",
			"url": "http://github.com/Mineru98"
		}
	],
	"engines": {
		"node": ">=0.8"
	},
	"main": "./lib/searchts.ts",
	"module": "src/searchts.ts",
	"files": [
		"/lib",
		"src"
	],
	"repository": {
		"type": "git",
		"url": "http://github.com/Mineru98/searchts.git"
	},
	"bugs": "https://github.com/Mineru98/searchts/issues",
	"devDependencies": {
		"@babel/core": "^7.5.5",
		"@babel/plugin-external-helpers": "^7.2.0",
		"@babel/preset-env": "^7.5.5",
		"@babel/register": "^7.5.5",
		"@rollup/plugin-typescript": "^8.2.5",
		"@types/chai": "^4.2.21",
		"@types/mocha": "^9.0.0",
		"@types/node": "^16.7.10",
		"chai": "^4.3.4",
		"codecov": "^3.6.5",
		"mocha": "^9.1.1",
		"nyc": "^12.0.2",
		"rollup": "^2.56.3",
		"should": "^13.2.1",
		"ts-node": "^10.2.1",
		"tslib": "^2.3.1",
		"typescript": "^4.4.2",
		"uglify-js": "^3.4.1"
	},
	"scripts": {
		"tsc": "rm -rf lib && tsc",
		"build": "rm -rf lib && NODE_ENV=prod rollup -c && yarn uglify",
		"prepublishOnly": "yarn build",
		"uglify": "uglifyjs ./lib/searchts.ts --compress --mangle --comments -o ./lib/searchts.min.js",
		"test": "NODE_ENV=test mocha -r ts-node/register ./test/test.ts --reporter spec",
		"test-watch": "NODE_ENV=test mocha --watch -r ts-node/register ./test/test.ts --reporter spec",
		"bundle": "rollup -c",
		"coverage": "nyc yarn test"
	},
	"dependencies": {}
}
